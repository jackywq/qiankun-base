{
  "name": "qiankun-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prestart && vite --port 8888",
    "start": "npm run dev & npm run start-react & npm run start-vue",
    "build": "vite build",
    "preview": "vite preview",
    "start-react": "cd ./qiankun-react && npm run start-no-open",
    "start-vue": "cd ./qiankun-vue && npm run serve",
    "install:all": "npm install && cd ./qiankun-react && npm install && cd ../qiankun-vue && npm install",
    "react-deploy": "cd ./qiankun-react && s deploy -y",
    "vue-deploy": "cd ./qiankun-vue && s deploy -y",
    "deploy": "s deploy -y &  npm run react-deploy & npm run vue-deploy",
    "prestart": "kill-port 8888"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "antd": "^5.18.0",
    "qiankun": "^2.3.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "kill-port": "^2.0.1",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
